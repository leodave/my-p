sed) {\n        this.$refs.videoPlayer.play();\n        console.log(\"plaay\");\n      } else {\n        this.$refs.videoPlayer.pause();\n      }\n    },\n    play() {\n      this.$refs.videoPlayer.play();\n      this.pausebtn.show();\n      this.playbtn.hide();\n    },\n    pause() {\n      this.$refs.videoPlayer.pause();\n      this.pausebtn.hide();\n      this.playbtn.show();\n    },\n  },\n};\n</script>\n<style>\n.bg-videoplayer {\n  position: absolute;\n  top: 30%;\n  left: 0;\n  min-width: 100%;\n  height: 80%;\n  overflow: hidden;\n}\n\n.bg-videoplayer video {\n  position: absolute;\n  top: 80%;\n  left: 100%;\n  min-width: 100%;\n  min-height: 300%;\n  width: auto;\n  height: 500px;\n  z-index: -100;\n  transform: translateX(-50%) translateY(-50%);\n}\n.football {\n  font-style: inherit;\n  font-weight: 20;\n  font-size: x-large;\n  font-family: \"Franklin Gothic Medium\", \"Arial Narrow\", Arial, sans-serif;\n\n  position: absolute;\n  left: 10%;\n  top: 10%;\n  width: 85%;\n}\n.plybtn {\n  width: 100;\n  height: 100;\n  left: 50%;\n  top: 50%;\n  color: blue;\n}\n.psebtn {\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}